{
  "name": "carbon-offset-calculator",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@smui-extra/autocomplete": "^7.0.0-beta.0",
    "@smui/button": "^7.0.0-beta.0",
    "@smui/chips": "^7.0.0-beta.0",
    "@smui/circular-progress": "^7.0.0-beta.0",
    "@smui/data-table": "^7.0.0-beta.0",
    "@smui/dialog": "^7.0.0-beta.0",
    "@smui/fab": "^7.0.0-beta.0",
    "@smui/icon-button": "^7.0.0-beta.0",
    "@smui/list": "^7.0.0-beta.0",
    "@smui/switch": "^7.0.0-beta.0",
    "@smui/tab": "^7.0.0-beta.0",
    "@smui/tab-bar": "^7.0.0-beta.0",
    "@smui/textfield": "^7.0.0-beta.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-netlify": "^2.0.6",
    "@sveltejs/kit": "^1.5.0",
    "smui-theme": "^7.0.0-beta.0",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "vite": "^4.0.0"
  },
  "type": "module",
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "lodash": "^4.17.21",
    "svelte-fa": "^3.0.3",
    "svelte-frappe-charts": "^1.10.0",
    "zero-md": "^2.4.0"
  }
}
